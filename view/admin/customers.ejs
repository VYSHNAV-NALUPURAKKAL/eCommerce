<section class="is-title-bar">
  <div class="flex flex-col md:flex-row items-center justify-between space-y-6 md:space-y-0">
    <ul>
      <li>Admin</li>
      <li>Customers</li>
    </ul>
  </div>
</section>
<section class="section main-section">
  <div class="grid gap-6 grid-cols-1 md:grid-cols-6 mb-4">
      <a href="/admin/customer" class="card">
          <div class="card-content">
              <div class="flex items-center justify-between">
                  <div class="widget-label">
                      <h3>
                          Customers
                      </h3>
                      <h1>
                          <%=customers.length %>
                      </h1>
                  </div>
                  <span class="icon widget-icon text-green-500"><i class="mdi mdi-account-multiple mdi-48px"></i></span>
              </div>
          </div>
      </a>
  </div>
            
    <div class="card has-table">
      <header class="card-header">
        <p class="card-header-title">
          <span class="icon"><i class="mdi mdi-account-multiple"></i></span>
          Customers
        </p>
        <a href="/admin/customers" class="card-header-icon">
          <span class="icon"><i class="mdi mdi-reload"></i></span>
        </a>
      </header>
      <div class="card-content">
        <table>
          <thead>
          <tr>
            <th></th>
            <th>Name</th>
            <th>E-mail</th>
            <th>Phone</th>
            <th>Actions</th>
          </tr>
          </thead>
          <tbody>
            <% if(customers.length>0){ %> <% customers.forEach((item,index)=>{ %>
          <tr>
            <td class="image-cell">
              <div class="image">
                <img src="https://avatars.dicebear.com/v2/initials/rebecca-bauch.svg" class="rounded-full">
              </div>
            </td>
            <td><%= item.name%></td>
            <td><%= item.email%></td>
            <td><%= item.mobile%></td>
            <td class="actions-cell">
              <div class="buttons right nowrap">
                <button 
                class="btn btn-block <%= item.blocked?'btn-danger':'btn-success' %>btn-block " 
                id="<%= item._id %>" 
                onclick="confirmShow(this)">
                  <!-- <span class="icon"><i class="mdi mdi-eye"></i></span> -->
                  <%= item.blocked?'Unblock':'Block' %>
                </button>
                <!-- <button class="button small red --jb-modal" data-target="sample-modal" type="button">
                  <span class="icon"><i class="mdi mdi-trash-can"></i></span>
                </button> -->
              </div>
            </td>
          </tr>
          <% })}else{ %>
          <tr>
            <td></td>
            <td colspan="3" class="">No customers found</td>
          </tr>
            <%}%>
          </tbody>
        </table>

        <!--Confirm Modal -->
<div
class="modal fade"
id="confirm-modal"
tabindex="-1"
aria-labelledby="exampleModalLabel"
aria-hidden="true"
>
<div class="modal-dialog">
  <div class="modal-content">
    <div class="modal-header">
      <h1 class="modal-title fs-5" id="exampleModalLabel">Are you sure?</h1>

      <button
        type="button"
        class="btn-close"
        data-bs-dismiss="modal"
        aria-label="Close"
      ></button>
    </div>
    <div class="modal-body">
      <p class="error-modal"></p>
      <p>
        Do you really want to <strong id="action-text">block</strong> this
        user?
      </p>
      <input type="hidden" name="id" id="action-id" value="" />
    </div>
    <div class="modal-footer">
      <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">
        Cancel
      </button>
      <button
        type="button"
        class="btn btn-primary"
        onclick="customerBlock()"
        id="action-btn"
      >
        Block
        </button>
      </div>
    </div>
  </div>
</section>
  <div class="table-pagination">
    <div class="flex items-center justify-between">
      <div class="buttons">
        <button type="button" class="button active">1</button>
        <button type="button" class="button">2</button>
        <button type="button" class="button">3</button>
      </div>
      <small>Page 1 of 3</small>
    </div>
  </div>

</div>
<style>
  .modal-backdrop{
    position: relative;
  }
</style>


